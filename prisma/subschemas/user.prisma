model User {
  id                Int                @id @default(autoincrement())
  name              String
  profileUrl        String
  country           String
  userNotifications UserNotification[]
  comments          Comment[]
  likedNews         LikedNews[]
  likedComments     LikedComment[]
  createdAt         DateTime           @default(now())
  updatedAt         DateTime           @updatedAt
}
