model City {
  id          Int           @id @default(autoincrement())
  name        String
  countryId   Int
  country     Country       @relation(fields: [countryId], references: [id], onDelete: Cascade)
  news        News[]
  userSetting UserSetting[]
  reporter    Reporter[]
  createdAt   DateTime      @default(now())
  updatedAt   DateTime      @updatedAt

  @@index([countryId])
}
